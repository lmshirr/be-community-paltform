{
  "name": "community-platform",
  "version": "1.0.0",
  "description": "To make it easy for you to get started with GitLab, here's a list of recommended next steps.",
  "main": "app/server.js",
  "scripts": {
    "build": "npm install",
    "development": "node app.js",
    "start:dev": "nodemon --watch app --watch docs app/server.js",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:undo": "npx sequelize-cli db:migrate:undo",
    "db:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "db:drop": "npx sequelize-cli db:drop",
    "db:create": "npx sequelize-cli db:create",
    "db:seed:all": "npx sequelize-cli db:seed:all",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json --watch"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.15.3",
    "axios": "^0.21.4",
    "bcrypt": "^5.0.1",
    "config": "^3.3.6",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "googleapis": "^85.0.0",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
    "nodemailer": "^6.6.2",
    "nodemailer-smtp-transport": "^2.7.4",
    "pg": "^8.6.0",
    "sequelize": "^6.6.2",
    "url-join": "^4.0.1",
    "uuid": "^8.3.2",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/joi": "^17.2.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "faker": "^5.5.3",
    "jest": "^27.2.5",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.4.0",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.6",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageDirectory": "./coverage",
    "verbouse": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/egyaranda02/community-platform.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/egyaranda02/community-platform/issues"
  },
  "homepage": "https://github.com/egyaranda02/community-platform#readme"
}
